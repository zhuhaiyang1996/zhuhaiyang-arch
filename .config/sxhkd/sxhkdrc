super + shift + p
    bash ~/.config/rofi/scripts/launch.sh powermenu

alt + m
    bash ~/.config/rofi/scripts/rofi.sh -e

super + c
    bash ~/.config/rofi/scripts/rofi.sh -c

super + shift + c
    copyq toggle

super + Insert
    notify-send "üìã Clipboard contents:" "$(xclip -o -selection clipboard)"

alt + p
    flameshot gui -p /home/zhuhaiyang/Documents/Pictures

alt + n
    nemo

alt + c
    code

alt + t
    typora

alt + b
    qutebrowser

alt + shift + t
    ~/.config/rofi/scripts/tmux_session.sh

super + plus
    /home/zhuhaiyang/.config/rofi/android/volume.sh

super + Return
    alacritty

super + space
    rofi -show drun -theme oxide

alt + Tab
    rofi -show window -theme awel

super + shift + o
    /home/zhuhaiyang/.local/bin/ocr.sh

# quit/restart bspwm
super + ctrl + {q,r}
	bspc {quit,wm -r}

# volume up
super + {equal,plus}
    pulsemixer --change-volume {+5,+15}
# volume down
super + {_,shift + } minus
	pulsemixer --change-volume {-5,-15}

# Multimedia keys
XF86AudioRaiseVolume
	amixer -q set Master 5%+ unmute

XF86AudioLowerVolume
	amixer -q set Master 5%- unmute

XF86AudioMute
    amixer set Master toggle

#Èü≥‰πêmpdÔºåmpc
super + less
    mpc prev

super + greater
    mpc next
